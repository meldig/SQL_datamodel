<userDefinedSQL><![CDATA[SELECT
	a.insee,
	a.nom,
	a.geom
FROM
	ta_commune a
	INNER JOIN ta_za_communes b
	ON a.fid_validite = b.objectid
WHERE
	sysdate BETWEEN b.debut_validite AND b.fin_validite;]]></userDefinedSQL>
