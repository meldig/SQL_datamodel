<ordsAlias>view_2</ordsAlias>
<userDefined>true</userDefined>
<userDefinedSQL><![CDATA[SELECT
	a.objectid AS id_ut,
	a.nom_complet,
	SDO_AGGR_UNION(
		SDOAGGRTYPE(
			b.geom,
			0.005
		)
	) AS geom

FROM
	ta_za_communes c
	INNER JOIN ta_commune b
	ON b.objectid = c.fid_commune
	INNER JOIN ta_zone_administrative a
	ON a.objectid = c.fid_zone_administrative
	
WHERE
	sysdate BETWEEN a.debut_validite AND a.fin_validite;]]></userDefinedSQL>
