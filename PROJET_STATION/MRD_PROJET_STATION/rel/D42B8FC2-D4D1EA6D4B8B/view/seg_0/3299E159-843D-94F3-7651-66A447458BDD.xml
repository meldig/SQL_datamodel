<userDefinedSQL><![CDATA[CREATE OR REPLACE FORCE VIEW G_DALC.V_CAPTEUR_5G_AGREGEES_QUOTIDIEN(
    OBJECTID,
    IDENTIFIANT_CAPTEUR,
    FID_IDENTIFIANT_CAPTEUR,
    GEOM,
    DATE_MESURE,
    POWER_MINIMUM,
    POWER_MAXIMUM,
    POWER_MOYENNE,
    TEMPERATURE_INTERNE_MINIMUM,
    TEMPERATURE_INTERNE_MAXIMUM,
    TEMPERATURE_INTERNE_MOYENNE,
    HUMIDITE_INTERNE_MINIMUM,
    HUMIDITE_INTERNE_MAXIMUM,
    HUMIDITE_INTERNE_MOYENNE,
    CONSTRAINT "V_CAPTEUR_5G_AGREGEES_QUOTIDIEN_PK" PRIMARY KEY ("OBJECTID") DISABLE) AS
        SELECT
            a.OBJECTID AS OBJECTID,
            b.IDENTIFIANT_CAPTEUR AS IDENTIFIANT_CAPTEUR,
            b.OBJECTID AS FID_IDENTIFIANT_CAPTEUR, 
            TO_DATE(a.DATE_MESURE, 'YYYY/MM/DD') AS DATE_MESURE,
            a.POWER_MINIMUM,
            a.POWER_MAXIMUM,
            a.POWER_MOYENNE,
            a.TEMPERATURE_INTERNE_MINIMUM,
            a.TEMPERATURE_INTERNE_MAXIMUM,
            a.TEMPERATURE_INTERNE_MOYENNE,
            a.HUMIDITE_INTERNE_MINIMUM,
            a.HUMIDITE_INTERNE_MAXIMUM,
            a.HUMIDITE_INTERNE_MOYENNE,
            b.GEOM AS GEOM
        FROM
            G_DALC.TA_CAPTEUR_5G_COLLECTE_AGREGEES_QUOTIDIEN a
            INNER JOIN G_DALC.TA_CAPTEUR_5G b ON a.FID_IDENTIFIANT_CAPTEUR = b.OBJECTID
;
]]></userDefinedSQL>
